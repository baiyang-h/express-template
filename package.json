{
  "name": "my-admin-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "server": "babel-node ./src/bin/www.js",
    "server:prod": "node ./dist/bin/www.js",
    "start": "pm2 start ./dist/bin/www.js",
    "build": "babel ./src --out-dir dist",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "prod": "cross-env NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon",
    "pm2": "pm2 start ./dist/bin/www.js",
    "pm2:stop-all": "pm2 stop all",
    "pm2:delete-all": "pm2 delete all"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.5.5",
    "rimraf": "^3.0.2",
    "swagger-jsdoc": "^7.0.0-rc.5",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  }
}
